game: main.o bootFunctions.o drawFunctions.o colors.o coordList.o boxList.o moveSprite.o sub.h
	gcc -g main.o bootFunctions.o drawFunctions.o colors.o coordList.o boxList.o moveSprite.o -I/usr/include/SDL2 -D_GNU_SOURCE=1 -D_REENTRANT -lSDL2 -lSDL2_image -lSDL2_ttf -lm -o game
main.o: main.c sub.h
	gcc -c main.c
bootFunctions.o: bootFunctions.c sub.h
	gcc -c bootFunctions.c
drawFunctions.o: drawFunctions.c sub.h
	gcc -c drawFunctions.c
colors.o: colors.c sub.h
	gcc -c colors.c
coordList.o: coordList.c sub.h
	gcc -c coordList.c
boxList.o: boxList.c sub.h
	gcc -c boxList.c
moveSprite.o: moveSprite.c sub.h
	gcc -c moveSprite.c
sub.h:

clean:
	rm game main.o bootFunctions.o drawFunctions.o colors.o coordList.o boxList.o moveSprite.o
